{
	"trailingComma": "es5",
	"tabWidth": 2,
	"printWidth": 80,
	"semi": true,
	"singleQuote": false,
	"useTabs": true,
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"bracketSpacing": true,
	"jsxBracketSameLine": true,
	"arrowParens": "avoid",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "preserve",
	"endOfLine": "lf"
}
